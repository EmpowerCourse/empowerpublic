
@model ActorListViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="jumbotron">
    <h1>Empower MVC training</h1>
    <h2>Paul Alan Taylor</h2>
</div>
<div class="row">
    @foreach (var actor in Model.Actors)
    {
    <div class="col-md-4">
        <h2>@actor.LastName, @actor.FirstName</h2>
    </div>
    }
</div>
